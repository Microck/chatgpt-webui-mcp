{
  "name": "chatgpt-webui-mcp",
  "version": "0.1.6",
  "description": "Standalone MCP server for ChatGPT Web UI via session token",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "INSTALL.md",
    "LICENSE",
    ".env.example",
    "deploy/systemd"
  ],
  "bin": {
    "chatgpt-webui-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "self-test": "tsx src/self-test.ts",
    "lint:commit": "commitlint --edit",
    "prepare": "husky"
  },
  "keywords": [
    "mcp",
    "chatgpt",
    "webui",
    "model-context-protocol",
    "session-token",
    "mcp-server",
    "camofox"
  ],
  "author": "Microck",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microck/chatgpt-webui-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/Microck/chatgpt-webui-mcp/issues"
  },
  "homepage": "https://github.com/Microck/chatgpt-webui-mcp#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.17.2",
    "husky": "^9.1.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
