# ChatGPT session cookie token
CHATGPT_SESSION_TOKEN=

# transport: camofox (recommended) or httpcloak
CHATGPT_TRANSPORT=camofox

# ChatGPT base URL
CHATGPT_WEBUI_BASE_URL=https://chatgpt.com

# camofox browser server
CHATGPT_CAMOFOX_BASE_URL=http://127.0.0.1:9377
CHATGPT_CAMOFOX_USER_ID=chatgpt-webui-mcp
CHATGPT_CAMOFOX_SESSION_KEY=chatgpt-webui
CHATGPT_CAMOFOX_WAIT_TIMEOUT_MS=5400000
CHATGPT_CAMOFOX_WORKSPACE=PRO

# if camofox cookie import is protected
CHATGPT_CAMOFOX_API_KEY=

# MCP server transport
MCP_TRANSPORT=sse
MCP_SSE_HOST=0.0.0.0
MCP_SSE_PORT=8791

# background run retention
CHATGPT_ASYNC_JOB_TTL_MS=86400000
CHATGPT_ASYNC_JOB_MAX=150
